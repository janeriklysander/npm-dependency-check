{
	"name": "npm-dependency-check",
	"displayName": "NPM Dependency Check",
	"description": "This plugin will help you make sure your local NPM packages are not out of date with the package.json specified versions.",
	"version": "1.0.0",
	"publisher": "janeriklysander",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:package.json"
	],
	"main": "./dist/extension.js",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"ts-loader": "^6.1.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9"
	},
	"dependencies": {
		"installed-check": "^3.0.0"
	}
}
